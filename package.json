{
  "name": "fillergame",
  "version": "0.0.0",
  "description": "Multiplayer game on matrix coloring",
  "author": "Eugene Sharygin <eush77@gmail.com>",
  "license": "MIT",
  "scripts": {
    "postinstall": "ln -fs ../src/js/game node_modules/_game",
    "make": "mkdir -p dist && npm run images && npm run html && npm run css && npm run js",
    "images": "cp -v img/* dist",
    "html": "jade <src/html/index.jade >dist/index.html",
    "css": "stylus <src/css/main.styl |cat node_modules/normalize.css/normalize.css - |autoprefixer |cleancss >dist/main.css",
    "js": "browserify src/js/client.js |uglifyjs >dist/index.js",
    "test": "tape test/*.js",
    "start": "node src/js/server.js"
  },
  "homepage": "https://github.com/eush77/fillergame",
  "repository": {
    "type": "git",
    "url": "https://github.com/eush77/fillergame"
  },
  "bugs": {
    "url": "https://github.com/eush77/fillergame/issues"
  },
  "keywords": [],
  "dependencies": {
    "algorithms": "^0.8.2",
    "canvas-grid": "^0.2.1",
    "cmpby": "^0.1.0",
    "color-rainbow": "0.0.0",
    "extend": "^2.0.0",
    "flatmap": "0.0.3",
    "fzip": "^0.2.0",
    "normalize.css": "^3.0.2",
    "uniq": "^1.0.1"
  },
  "devDependencies": {
    "autoprefixer": "^4.0.0",
    "browserify": "^7.0.1",
    "clean-css": "^2.2.22",
    "jade": "^1.8.1",
    "node-static": "^0.7.6",
    "stylus": "^0.49.3",
    "tape": "^3.0.3",
    "thus": "^1.0.0",
    "uglify-js": "^2.4.16"
  }
}
